<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="preprocessor-upper" name="upper" class="edu.school21.preprocessor.PreProcessorToUpperImpl"/>
    <bean id="preprocessor-lower" name="lower" class="edu.school21.preprocessor.PreProcessorToLowerImpl"/>



    <bean id="renderer-err-lower" name="el" class="edu.school21.renderer.RendererErrImpl">
        <constructor-arg name="preProcessor" ref="preprocessor-lower"/>
    </bean>

    <bean id="renderer-err-upper" name="eup" class="edu.school21.renderer.RendererErrImpl">
        <constructor-arg name="preProcessor" ref="preprocessor-upper"/>
    </bean>

    <bean id="renderer-std-upper" name="sup" class="edu.school21.renderer.RendererStandardImpl">
        <constructor-arg name="preProcessor" ref="preprocessor-upper"/>
    </bean>

    <bean id="renderer-std-lower" name="sl" class="edu.school21.renderer.RendererStandardImpl">
        <constructor-arg name="preProcessor" ref="preprocessor-lower"/>
    </bean>


    <bean id="printer-date-err-lower" name="pdel" class="edu.school21.printer.PrinterWithDateTimeImpl">
        <constructor-arg name="renderer" ref="renderer-err-lower"/>
    </bean>

    <bean id="printer-date-err-upper" name="pdeup" class="edu.school21.printer.PrinterWithDateTimeImpl">
        <constructor-arg name="renderer" ref="renderer-err-upper"/>
    </bean>

    <bean id="printer-date-std-lower" name="pdsl" class="edu.school21.printer.PrinterWithDateTimeImpl">
        <constructor-arg name="renderer" ref="renderer-std-lower"/>
    </bean>

    <bean id="printer-date-std-upper" name="pdsup" class="edu.school21.printer.PrinterWithDateTimeImpl">
        <constructor-arg name="renderer" ref="renderer-std-upper"/>
    </bean>



    <bean id="printer-prefix-err-lower" name="ppel" class="edu.school21.printer.PrinterWithPrefixImpl">
        <constructor-arg name="renderer" ref="renderer-err-lower"/>
    </bean>

    <bean id="printer-prefix-err-upper" name="ppeup" class="edu.school21.printer.PrinterWithPrefixImpl">
        <constructor-arg name="renderer" ref="renderer-err-upper"/>
    </bean>


    <bean id="printer-prefix-std-lower" name="ppsl" class="edu.school21.printer.PrinterWithPrefixImpl">
        <constructor-arg name="renderer" ref="renderer-std-lower"/>
    </bean>

    <bean id="printer-prefix-std-upper" name="ppsup" class="edu.school21.printer.PrinterWithPrefixImpl">
        <constructor-arg name="renderer" ref="renderer-std-upper"/>
    </bean>


</beans>